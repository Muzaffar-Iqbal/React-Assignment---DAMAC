  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "shx rm -rf build dist && shx echo Clean Done && mkdir build && mkdir dist",
    "build:client": "webpack --env.BASE_URL=https://localhost:3001/ --env.ENV_TYPE=production --env.API_URL_DEVELOPMENT=http://localhost:3001/api/ --env.API_URL_PRODUCTION=https://localhost:3001/api/ --mode production --config ./config/webpack.prod.config.js && shx cp -rf public/images build/images && shx cp -rf public/css build/css && shx cp -rf public/fonts build/fonts",
    "build:server": "babel server -d dist",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=development webpack-dev-server --env.API_URL=http://localhost:3001/api/ --open --mode development --config ./config/webpack.dev.config.js",
    "start:server": "cross-env NODE_ENV=development nodemon --watch server server/index.js --exec node",
    "serve": "cross-env NODE_ENV=production node dist/index.js"
  },
